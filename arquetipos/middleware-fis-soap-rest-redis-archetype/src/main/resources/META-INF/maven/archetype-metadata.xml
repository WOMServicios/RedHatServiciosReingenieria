		<defaultValue>6379</defaultValue>
        </requiredProperty>
	<requiredProperty key="soapPort">
		<defaultValue>8888</defaultValue>
        </requiredProperty>
	<requiredProperty key="restPort">
		<defaultValue>8889</defaultValue>
        </requiredProperty>
        <requiredProperty key="soapContext">
